# Environment Variables Example
# Copy this file to .env.local and fill in your actual values

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://doingoksocial.com
NEXT_PUBLIC_SITE_NAME="Doing OK Social Club"

# Contact Information
NEXT_PUBLIC_CONTACT_EMAIL=hello@doingoksocial.com
NEXT_PUBLIC_CONTACT_PHONE="+1-405-555-DOINGOK"

# Location
NEXT_PUBLIC_ADDRESS="123 Main Street, Oklahoma City, OK 73102"
NEXT_PUBLIC_LATITUDE=35.4676
NEXT_PUBLIC_LONGITUDE=-97.5164

# Social Media
NEXT_PUBLIC_INSTAGRAM_URL=https://instagram.com/doingoksocial
NEXT_PUBLIC_FACEBOOK_URL=https://facebook.com/doingoksocial
NEXT_PUBLIC_TWITTER_URL=https://twitter.com/doingoksocial

# Analytics (Optional)
NEXT_PUBLIC_GA_TRACKING_ID=
NEXT_PUBLIC_GTM_ID=

# Email Service (Optional - for contact form)
EMAILJS_SERVICE_ID=
EMAILJS_TEMPLATE_ID=
EMAILJS_PUBLIC_KEY=

# Database (Optional - for events/bookings)
DATABASE_URL=

# Payment Processing (Optional - for ticket sales)
STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=

# Map Integration (Optional)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=
